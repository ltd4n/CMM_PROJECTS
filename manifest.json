{
  "manifest_version": 3,
  "name": "Project Tracker",
  "version": "1.0",
  "description": "A Chrome extension to track projects and save data to Google Sheets.",
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
